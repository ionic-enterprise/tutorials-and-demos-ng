{
  "name": "demo-pwa",
  "description": "A Progressive Web App example",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "android": "cap open android",
    "assets": "capacitor-assets generate --iconBackgroundColor '#f1ebe1' --splashBackgroundColor '#f1ebe1' --iconBackgroundColorDark '#110b00' --splashBackgroundColorDark '#110b00'",
    "build": "ng build --configuration production && cap sync",
    "ios": "cap open ios",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve --port=8100",
    "test": "ng test --no-watch",
    "test:debug": "ng test --browsers=Chrome",
    "test:dev": "ng test",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.10",
    "@angular/core": "^19.2.10",
    "@angular/forms": "^19.2.10",
    "@angular/platform-browser": "^19.2.10",
    "@angular/platform-browser-dynamic": "^19.2.10",
    "@angular/router": "^19.2.10",
    "@angular/service-worker": "^19.2.10",
    "@capacitor/android": "^7.2.0",
    "@capacitor/app": "^7.0.1",
    "@capacitor/core": "^7.2.0",
    "@capacitor/haptics": "^7.0.1",
    "@capacitor/ios": "^7.2.0",
    "@capacitor/keyboard": "^7.0.1",
    "@capacitor/preferences": "^7.0.1",
    "@capacitor/privacy-screen": "^1.1.0",
    "@capacitor/share": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@ionic-enterprise/auth": "7.1.1",
    "@ionic-enterprise/identity-vault": "~6.0.2",
    "@ionic/angular": "~8.5.7",
    "ionicons": "~8.0.8",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.12",
    "@angular/cli": "^19.2.12",
    "@angular/compiler": "^19.2.10",
    "@angular/compiler-cli": "^19.2.10",
    "@angular/language-service": "^19.2.10",
    "@capacitor/assets": "~3.0.5",
    "@capacitor/cli": "^7.2.0",
    "@ionic/angular-toolkit": "~12.2.0",
    "@types/jasmine": "~5.1.8",
    "@types/node": "^22.15.18",
    "angular-eslint": "^19.4.0",
    "eslint": "^9.26.0",
    "jasmine-core": "~5.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-node": "~10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.1"
  }
